<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RestaurantSuite">
	<listeners>
		<listener class-name="com.POS.Utilities.Listeners"></listener>
	</listeners>
	<parameter name="Browser" value="Edge"></parameter>
	<parameter name="ZoneName" value="Test OBS Zone"></parameter>
	<parameter name="StoreTblNm" value="Test OBS Store Table"></parameter>
	<test thread-count="5" name="Test">
		<classes>
			<class name="com.POS.Testcases.VerifyUserLoginTest" />
			<class name="com.POS.Testcases.VerifyHomePageTest" />
			<class name="com.POS.Testcases.VerifyProductPageTest" />
			<class name="com.POS.Testcases.VerifyStoreTest" />
			<class name="com.POS.Testcases.VerifyStoreManageTest" />
			<class name="com.POS.Testcases.VerifyWaiterTest" />
			<class name="com.POS.Testcases.VerifyCategoryTest" />
			<class name="com.POS.Testcases.VerifySettingsTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- RestaurantSuite -->
